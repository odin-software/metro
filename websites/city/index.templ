			<script type="module" src="/js/viewport.js"></script>
			<script type="module" src="/js/world.js"></script>
			<script type="module" src="/js/load.js"></script>
			<script type="module" src="/js/utils/consts.js"></script>
			<script type="module" src="/js/main.js"></script>
		</body>

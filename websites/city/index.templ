package city

templ Index() {
	<html>
		<head>
			<title>City</title>
			<link rel="stylesheet" href="/css/index.css"/>
		</head>
		<body>
			// Canvas
			<h1>City</h1>
			<canvas id="cityCanvas"></canvas>

package city

templ Editor() {
	<html>
		<head>
			<title>City Editor</title>
			<link rel="stylesheet" href="/css/editor.css"/>
			<link rel="stylesheet" href="/css/dialog.css"/>
		</head>
		<body>
			// Canvas
			<h1>City Editor</h1>
			<canvas id="editorCanvas"></canvas>
			<div id="dialog" class="dialog">
				<div class="dialog-body">
					<h1 class="dialog-title">A custom modal popup</h1>
					<p class="dialog-text"></p>
					<input id="dialogInput" type="text"/>
					<button id="yesBtn" type="button" class="dialog-yes">yes</button>
					<button id="noBtn" type="button" class="dialog-no">no</button>
				</div>
			</div>
			<div id="trainDiv">
				<ul id="trainList"></ul>
			</div>
			<div id="controls">
				<button id="saveBtn">💾</button>
				&nbsp;
				<button id="graphBtn">️🌐</button>
				<button id="lineBtn">️🖊️</button>
				<div id="osmPanel" style="display:none;">
					<textarea id="osmDataContainer" placeholder="Paste OSM data here" rows="10" cols="50"></textarea>
					<div>
						<button onclick="loadOsmData()">✔️</button>
						<button onclick="closeOsmPanel()">❌</button>
					</div>
				</div>
			</div>
			<script type="module" src="/js/primitives/point.js"></script>
			<script type="module" src="/js/primitives/segment.js"></script>
			<script type="module" src="/js/math/graph.js"></script>
			<script type="module" src="/js/math/utils.js"></script>
			<script type="module" src="/js/viewport.js"></script>
			<script type="module" src="/js/world.js"></script>
			<script type="module" src="/js/load.js"></script>
			<script type="module" src="/js/utils/consts.js"></script>
			<script type="module" src="/js/editors/networkEditor.js"></script>
			<script type="module" src="/js/emain.js"></script>
		</body>
	</html>
}

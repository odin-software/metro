			<script src="/vw-js/editors/markingEditor.js"></script>
			<script src="/vw-js/editors/stopEditor.js"></script>
			<script src="/vw-js/editors/crossingEditor.js"></script>
			<script src="/vw-js/editors/startEditor.js"></script>
			<script src="/vw-js/editors/parkingEditor.js"></script>
